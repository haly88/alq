<div class="panelAcciones">
  <%= boton_atras_root %><%= boton_nuevo %>
</div> 
<table class="dataTable display responsive no-wrap">
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nombre</th>
      <th>Descripcion</th>
    </tr>
  </thead>

  <tbody>
    <% @cajas_conceptos.each do |cajas_concepto| %>
      <tr>
        <td><%= link_to cajas_concepto.codigo, edit_cajas_concepto_path(cajas_concepto) %></td>
        <td><%= cajas_concepto.nombre %></td>
        <td><%= cajas_concepto.descripcion %></td>
      </tr>
    <% end %>
  </tbody>
</table>
