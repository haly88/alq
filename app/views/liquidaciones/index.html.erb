<table class="dataTable display responsive no-wrap">
  <thead>
    <tr>
      <th>Contrato</th>
      <th>Fecha</th>
      <th>Neto</th>
      <th>Descuento</th>
      <th>Comision</th>
      <th>Total</th>
    </tr>
  </thead>

  <tbody>
    <% @liquidaciones.each do |liquidacion| %>
      <tr>
        <td><%= link_to liquidacion.contrato.nombre, edit_liquidacion_path(liquidacion) %></td>
        <td><%= liquidacion.fecha %></td>
        <td><%= liquidacion.neto %></td>
        <td><%= liquidacion.descuento %></td>
        <td><%= liquidacion.comision %></td>
        <td><%= liquidacion.total %></td>
      </tr>
    <% end %>
  </tbody>
</table>

